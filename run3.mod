$PROBLEM  FIRST ADVAN3 -- POPULATION DATA
$INPUT      ID TIME CP=DV DOSE=AMT MDV
$DATA       test_data.txt 

$SUBROUTINES  ADVAN2 

$PK
;THETA(1)=MEAN ELIMINATION RATE CONSTANT K (1/HR)
;THETA(2)=VOLUME OF DISTRIBUTION
;THETA(3)=MEAN ABSORPTION RATE CONSTANT KA (1/HR)
CALLFL=1
K=THETA(1)+ETA(1) 
KA=THETA(2)+ETA(2)   ;ETA(1) - INTERINDIVIDUAL VARIABILITY OF KA (NU)
S2= THETA(3)+ETA(3)
;CL=K*V;


$THETA (.001,.7,5)  (.001,1,10)  (.001,0.3,20)
$OMEGA BLOCK(3)  .1   .0001 .01   .0001 .0001 .1   ;VARIANCE OF ETA

$ERROR
Y=F+EPS(1)

$SIGMA  .3                              ;VARIANCE OF EPS     


$COV                                    ;COVARIANCE STEP IS IMPLEMENTED
$TABLE ID TIME DOSE PRED DV FILE=tab003 NOPRINT
$SCAT           (DV PRED) VS TIME BY ID


$EST MAXEVAl=450 NOABORT PRINT=5 SIG=7
;$SIM (123)

$TABLE ID TIME DV FILE=tab3 NOPRINT ONEHEADER

