;; 1. Based on: run1
;; 2. Description: 1-comp iv, linear elimINTRAVENOUS BOLUS STUDY COVARIATE MODEL
;; x1. Author: user
;; 3. Label:

$PROBLEM PK

$INPUT ID TIME CONC=DV AMT DOSE MDV AGE WT SCR ISM CLCR

$DATA CS1_IV1EST_PAR.csv IGNORE=C

$SUBROUTINES ADVAN1 TRANS2

$PK
CL = THETA(1) * EXP(ETA(1))
V  = THETA(2) * EXP(ETA(2))
S1 = V

$ERROR
IPRED = F
;W = SQRT(THETA(3)**2*IPRED**2 + THETA(4)**2)
Y = IPRED*(1 +EPS(1))+EPS(2)
IRES = DV-IPRED
IWRES = IRES

$THETA
(0.1, 1)  ; CL
(1, 10, 20)  ; V

$OMEGA
0.09 ; ETA(1)
0.09 ; ETA(2)

$SIGMA
0.09 ;EPS(1)
1    ;EPS(2)

$EST METHOD=0 MAXEVAL=9999 PRINT=5 POSTHOC
$COV
;$COVARIANCE provides standard error or precision of estimates

; Xpose
$TABLE ID TIME DV IPRED IRES IWRES ONEHEADER NOPRINT FILE=sdtab3.tab
$TABLE ID CL V ETA1 ETA2 ONEHEADER NOPRINT NOAPPEND FILE=patab3.tab
$TABLE ID ISM ONEHEADER NOPRINT FILE=catab3.tab
$TABLE ID AGE WT CLCR ONEHEADER NOPRINT FILE=cotab3.tab

